{
  "name": "texteditor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "concurrently \"cd develop/server && npm run server\" \"cd develop/client && npm run start\"",
    "start": "npm run build && cd develop/server && node server.js",
    "server": "cd server nodemon server.js --ignore client",
    "build": "cd develop/client && npm run build",
    "install": "cd develop/server && npm i && cd ../client && npm i",
    "client": "cd develop/client && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kshakely5280/textEditor.git"
  },
